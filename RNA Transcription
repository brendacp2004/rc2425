rna_transcription(Rna, Dna) :-
  string_chars(Rna, RnaList),
  do_rna_transcription(RnaList, DnaList),
  string_chars(Dna, DnaList).
do_rna_transcription([], []) :- !.
do_rna_transcription([R|Rna], [D|Dna]) :-
  convert(R, D),
  do_rna_transcription(Rna, Dna).
convert('G', 'C').
convert('C', 'G').
convert('T', 'A').
convert('A', 'U').
